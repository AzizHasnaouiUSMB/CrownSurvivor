<UserControl x:Class="CrownSurvivor.UCParametres"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CrownSurvivor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/image/BackgroundRegles.png"/>
        </Grid.Background>

        <Button x:Name="butRetourPara" Content="RETOUR" HorizontalAlignment="Center" Margin="0,350,0,0" VerticalAlignment="Top" Height="77" Width="100" Foreground="#FFE6C18A">
            <Button.Background>
                <ImageBrush ImageSource="/image/BoutonFond.png"/>
            </Button.Background>
            <Button.OpacityMask>
                <ImageBrush ImageSource="/image/button.png"/>
            </Button.OpacityMask>
        </Button>

    </Grid>
</UserControl>
